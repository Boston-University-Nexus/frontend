ISPLAYED CLASSES */}\n        <div className=\"flex flex-col flex-grow w-full overflow-y-scroll\">\n          {/* IF CLASSES */}\n          {classList.map((item, key) => {\n            return <ClassItem item={item} key={key} />;\n          })}\n\n          {/* IF EMPTY SEARCH */}\n          {classList.length == 0 && this.state.typedText.length < 1 && (\n            <StartTyping />\n          )}\n\n          {/* IF NOT FOUND */}\n          {classList.length == 0 && this.state.typedText.length > 0 && (\n            <NotFound />\n          )}\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default connect(mapStateToProps, mapDispatchToProps)(ClassList);\n","/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/ClassSection/NotFound.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/ClassSection/StartTyping.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/ClassSection/ClassItem.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/ClassSection/ClassRating.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/ClassSection/SearchBar.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/Calendar/Calendar.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/Calendar/CalendarItem.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/Calendar/CalendarGraph.js",["118","119","120"],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/Calendar/CalendarStats.js",[],{"ruleId":"121","replacedBy":"122"},{"ruleId":"123","replacedBy":"124"},{"ruleId":"125","severity":1,"message":"126","line":3,"column":50,"nodeType":"127","messageId":"128","endLine":3,"endColumn":54},{"ruleId":"125","severity":1,"message":"129","line":3,"column":27,"nodeType":"127","messageId":"128","endLine":3,"endColumn":33},{"ruleId":"125","severi