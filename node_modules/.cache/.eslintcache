 <Route exact path=\"/planner\">\n            <PlannerRouter />\n          </Route>\n          <Route exact path=\"/coursesearch\">\n            <CourseSearchRouter />\n          </Route>\n          <Route>\n            <NotFound />\n          </Route>\n        </Switch>\n      </div>\n    </Router>\n  );\n}\n\nexport default App;\n","/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/state/store.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/state/reducers.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/state/constants.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Landing/Landing.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/routers/PlannerRouter.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/routers/CourseSearchRouter.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/NotFound/NotFound.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/Main.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/state/actions.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/Recommended.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/components/Planner/PlannerNav.js",["133","134","135"],"import React, { Component } from \"react\";\n\nimport { BrowserRouter as Router, Switch, Route, Link } from \"react-router-dom\";\n\nexport default class PlannerNav extends Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      menuOpen: false,\n    };\n  }\n  render() {\n    return (\n      <div className=\"w-full py-4 flex justify-center items-center bg-white shadow-xl absolute\">\n        <div className=\"w-full px-8 flex justify-between items-center\">\n          <div className=\"font-bold text-xl\">\n            <a href=\"/\">BUNexus</a>\n          </div>\n          <div className=\"flex items-center\">\n            <Link to=\"/planner\" className=\"text-blue-500 hover:underline mx-2\">\n              Planner\n            </Link>\n            <Link\n              to=\"/planner/about\"\n              className=\"text-blue-500 hover:underline mx-2\"\n            >\n              About\n            </Link>\n            <div\n              className=\"relative\"\n              onMouseEnter={() => this.setState({ menuOpen: true })}\n              onMouseLeave={() => this.setState({ menuOpen: false })}\n            >\n              <div className=\"w-10 h-10 bg-green-500 rounded-full ml-5\"></div>\n              {this.state.menuOpen && (\n                <div className=\"absolute flex flex-col bg-white shadow-xl rounded border border-gray-300 right-0\">\n                  <Link\n                    to=\"/planner/login\"\n                    className=\"px-4 py-2 hover:bg-gray-200 hover:text-blue-500\"\n                  >\n                    Login\n                  </Link>\n                  <Link\n                    to=\"/planner/profile\"\n                    className=\"px-4 py-2 hover:bg-gray-200 hover:text-blue-500\"\n                  >\n                    Profile\n                  </Link>\n                  <Link\n                    to=\"/planner/settings\"\n                    className=\"px-4 py-2 hover:bg-gray-200 hover:text-blue-500\"\n                  >\n                    Settings\n                  </Link>\n                </div>\n              )}\n            </div>\n          </div>\n        </div>\n      </div>\n    );\n  }\n}\n","/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/Methods.js",["136","137","138","139"],"// Filters classes according to text typed\nexport function filter(numWords, classes, val) {\n  if (val[0] == \"\") return [];\n\n  let currentClasses;\n\n  if (numWords == 1)\n    currentClasses = classes.filter(function (el) {\n      return (\n        el.college.toLowerCase().includes(val[0]) ||\n        el.department.toLowerCase().includes(val[0]) ||\n        el.number.toLowerCase().includes(val[0])\n      );\n    });\n  else if (numWords == 2)\n    currentClasses = classes.filter(function (el) {\n      return (\n        (el.college.toLowerCase().includes(val[0]) &&\n          el.department.toLowerCase().includes(val[1])) ||\n        (el.department.toLowerCase().includes(val[0]) &&\n          el.number.toLowerCase().includes(val[1]))\n      );\n    });\n  else if (numWords == 3)\n    currentClasses = classes.filter(function (el) {\n      return (\n        el.college.toLowerCase().includes(val[0]) &&\n        el.department.toLowerCase().includes(val[1]) &&\n        el.number.toLowerCase().includes(val[2])\n      );\n    });\n  else currentClasses = classes;\n\n  return currentClasses;\n}\n","/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/ClassSection/ClassList.js",["140","141"],"import React, { Component } from \"react\";\nimport { connect } from \"react-redux\";\nimport ClassItem from \"./ClassItem\";\nimport { filterClasses } from \"../../../state/actions\";\nimport { filter } from \"../Methods\";\n\nimport NotFound from \"./NotFound\";\nimport StartTyping from \"./StartTyping\";\nimport SearchBar from \"./SearchBar\";\n\nconst mapStateToProps = (state) => {\n  return {\n    displayedClasses: state.displayedClasses,\n    classes: state.classes,\n  };\n};\n\nconst mapDispatchToProps = (dispatch) => {\n  return {\n    filterClasses: (classes) => dispatch(filterClasses(classes)),\n  };\n};\n\nclass ClassList extends Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      hasTyped: false,\n      typedText: \"\",\n    };\n\n    this.handleType = this.handleType.bind(this);\n  }\n\n  handleType(e) {\n    // Is used to determine if we show \"not found\" or \"start typing\"\n    this.setState({ typedText: e.target.value });\n\n    let numWords = e.target.value.split(\" \").length;\n    let val = e.target.value.toLowerCase().split(\" \");\n    let currentClasses = filter(numWords, this.props.classes, val);\n\n    this.props.filterClasses(currentClasses);\n  }\n\n  render() {\n    let classList = this.props.displayedClasses.slice(0, 10);\n\n    return (\n      <div className=\"bg-white shadow-xl flex flex-col w-full mb-4 flex-grow overflow-hidden\">\n        <SearchBar handleType={this.handleType} />\n\n        {/* INDICATES WHAT EACH ITEM IS */}\n        <div className=\"bg-white py-5 pl-5 pr-3 flex uppercase font-bold text-gray-500 border-b border-gray-300 text-xs lg:text-base\">\n          <p className=\"w-3/5\">course</p>\n          <p className=\"w-1/5 text-center\">qual</p>\n          <p className=\"w-1/5 text-center\">diff</p>\n        </div>\n\n        {/* DISPLAYED CLASSES */}\n        <div className=\"flex flex-col flex-grow w-full overflow-y-scroll\">\n          {/* IF CLASSES */}\n          {classList.map((item, key) => {\n            return <ClassItem item={item} key={key} />;\n          })}\n\n          {/* IF EMPTY SEARCH */}\n          {classList.length == 0 && this.state.typedText.length < 1 && (\n            <StartTyping />\n          )}\n\n          {/* IF NOT FOUND */}\n          {classList.length == 0 && this.state.typedText.length > 0 && (\n            <NotFound />\n          )}\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default connect(mapStateToProps, mapDispatchToProps)(ClassList);\n","/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/ClassSection/NotFound.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/ClassSection/StartTyping.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/ClassSection/ClassItem.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/ClassSection/ClassRating.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/ClassSection/SearchBar.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/Calendar/Calendar.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/Calendar/CalendarStats.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/Calendar/StatsRating.js",[],"/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/Calendar/CalendarGraph/CalendarGraph.js",["142","143","144","145","146","147"],"import React, { Component } from \"react\";\nimport { connect } from \"react-redux\";\n\nimport CalendarItem from \"./CalendarItem\";\nimport { getCellHeight } from \"./CalendarMethods\";\n\nconst daysOfWeek = [\"monday\", \"tuesday\", \"wednesday\", \"thursday\", \"friday\"];\n\nconst mapStateToProps = (state) => {\n  return {\n    calendars: state.calendars,\n    activeSections: state.activeSections,\n  };\n};\n\nconst compareArrOfObj = (arr1, arr2) => {\n  if (arr1.length !== arr2.length) return false;\n\n  for (let i = 0; i < arr1.length; i++) {\n    if (\n      arr1[i].title != arr2[i].title ||\n      arr1[i].displayed != arr2[i].displayed\n    ) {\n      return false;\n    }\n  }\n\n  return true;\n};\n\n// Will probably be replaced by a library\nclass CalendarGraph extends Component {\n  constructor(props) {\n    super(props);\n\n    this.state = {\n      activeSections: {}, // The current calendar displayed\n      sections: {}, // A dictionary containing all section items\n      cellHeight: null,\n    };\n\n    this.hours = this.hours.bind(this);\n    this.days = this.days.bind(this);\n    this.generateSections = this.generateSections.bind(this);\n\n    this.calendarStart = 7; // Earliest hour displayed\n    this.calendarEnd = 19; // Latest hour displayed\n\n    this.calendarRef = React.createRef();\n  }\n\n  // Returns a cell for every hour in a day\n  hours() {\n    let arr = [];\n    for (let i = this.calendarStart; i < this.calendarEnd; i++) {\n      arr.push(\n        <div\n          className={\n            i == this.calendarEnd - 1 ? \"\" : \"border-b border-gray-300\"\n          }\n          style={{ height: this.state.cellHeight + \"px\" }}\n          key={i}\n        ></div>\n      );\n    }\n\n    return arr;\n  }\n\n  // Returns a day for every day of the week (with all hours in it)\n  days() {\n    let arr = [];\n    for (const i of daysOfWeek) {\n      arr.push(\n        <div className=\"flex flex-col w-full relative\" key={i}>\n          <div\n            className=\"uppercase text-gray-600 flex items-center justify-center text-sm xl:text-base\"\n            style={{ height: this.state.cellHeight + \"px\" }}\n          >\n            {i}\n          </div>\n          <div\n            className={\n              \"flex flex-col \" +\n              (i == daysOfWeek[0]\n                ? \"border border-gray-300\"\n                : \"border border-l-0 border-gray-300\")\n            }\n          >\n            {this.hours()}\n          </div>\n          {this.state.sections[i]}\n        </div>\n      );\n    }\n\n    return arr;\n  }\n\n  // Returns the hour text next to the scheduler\n  hourLeyend() {\n    let arr = [];\n    for (let i = this.calendarStart; i <= this.calendarEnd; i++) {\n      let text = i % 12;\n      if (i > 12) text += \" PM\";\n      else if (i == 12) text = \"12 PM\";\n      else text += \" AM\";\n\n      arr.push(\n        <div\n          className=\"w-full flex items-center justify-end text-gray-600\"\n          style={{ height: this.state.cellHeight + \"px\" }}\n          key={i}\n        >\n          {text}\n        </div>\n      );\n    }\n\n    return arr;\n  }\n\n  componentDidUpdate() {\n    // Only update if active calendar changed\n    let same = compareArrOfObj(\n      this.state.activeSections,\n      this.props.activeSections\n    );\n\n    if (!same) {\n      // Save state so we know what the current calendar is\n      this.setState(\n        {\n          activeSections: JSON.parse(JSON.stringify(this.props.activeSections)),\n        },\n        () => this.generateSections(this.state.activeSections)\n      );\n    }\n  }\n\n  // On resize this is called\n  resize() {\n    let cellHeight = getCellHeight(window.innerWidth);\n    this.setState({ cellHeight });\n  }\n\n  componentWillMount() {\n    // Gets cell height according to current window width\n    let cellHeight = getCellHeight(window.innerWidth);\n    window.addEventListener(\"resize\", this.resize.bind(this));\n\n    let sectionsList = JSON.parse(JSON.stringify(this.props.activeSections));\n\n    this.setState({ cellHeight });\n    this.generateSections(sectionsList);\n  }\n\n  // Generates all the calendar sections in their corresponding day\n  generateSections(sectionsList) {\n    let sections = {\n      monday: [],\n      tuesday: [],\n      wednesday: [],\n      thursday: [],\n      friday: [],\n    };\n\n    sectionsList.map((section) => {\n      if (!(section.displayed === false))\n        return section.days.map((day) => {\n          return sections[day].push(\n            <CalendarItem\n              section={section}\n              h={this.state.cellHeight}\n              key={section.id}\n              start={this.calendarStart}\n            />\n          );\n        });\n    });\n\n    this.setState({ sections });\n  }\n\n  render() {\n    return (\n      <div className=\"flex mt-10 w-full\">\n        <div className=\"flex w-full h-full overflow-hidden\">\n          <div\n            className=\"flex flex-col w-1/12 mr-1 text-xs xl:mt-0 xl:text-sm\"\n            style={{ marginTop: this.state.cellHeight / 2 + \"px\" }}\n          >\n            {this.hourLeyend()}\n          </div>\n          <div className=\"flex w-11/12 h-full\" ref={this.calendarRef}>\n            {this.days()}\n          </div>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default connect(mapStateToProps)(CalendarGraph);\n","/home/danielmelchor/Documents/CodeProjects/nexus/frontend/src/pages/Planner/Calendar/CalendarGraph/CalendarItem.js",[],"/home/danielmel